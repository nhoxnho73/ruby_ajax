<h2>Import</h2>
<div class="row">
<%= form_tag import_genres_path, multipart: true, class: "form-horizontal" do %>  
  <div class="form-group">
    <label for='import' class="col-sm-3 control-label">Import</label>
    <div class="col-sm-9">
      <%= file_field_tag :file, class: "form-control-file" %>
    </div>
  </div>

  <div class="form-group">
    <div class="col-sm-3"></div>
    <div class="col-sm-6">
      <button type='submit' class="form-control btn btn-warning">Import</button>
    </div>
  </div>
<% end %>
</div>
